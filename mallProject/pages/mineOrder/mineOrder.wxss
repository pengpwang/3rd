page{
  background: #f5f6fb;
  font-family:PingFang-SC-Medium;
}

.tab-wrap{
  width: 100%;
  height: 72rpx;
  background: #fff;
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-bottom: 16rpx;
}

.tab{
  font-size: 26rpx;
  color: #333;
}

.tab-active{
  color: #FF4648;
  border-bottom: 4rpx solid #FF4648;
}

.res-lists-pannel{

}

.res-lists-item{
  background: #fff;
  margin-bottom: 15rpx;
}

.res-lists-i-order-s{
  padding: 26rpx 20rpx 22rpx 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  color: #292621;
  background: #fff;
}

.res-lists-i-status{
  color: #FF3333;
}

.res-lists-i-info{
  background: #F7F7F7;
  height: 197rpx;
  padding: 21rpx 29rpx 26rpx 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.res-lists-i-pro{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: flex-start;
}

.res-lists-i-pro-pic{
  width: 150rpx;
  height: 150rpx;
  background: #fff;
}

.res-lists-i-pro-txt{
  font-size: 28rpx;
  color: #000;
  line-height: 42rpx;
  width: 390rpx;
  margin-left: 32rpx;
}

.res-lists-i-price{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-end;
}

.res-lists-i-price-1{
  font-size: 26rpx;
  color: #292621;
}

.res-lists-i-price-2{
  font-size: 26rpx;
  color: #8A8A8A;
  text-decoration: line-through;
}

.res-lists-i-price-3{
  font-size: 24rpx;
  color: #8A8A8A;
}

.res-lists-i-actually{
  padding-left: 30rpx;
  padding-right: 30rpx;
  width: 100%;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  font-size: 24rpx;
  color: #292621;
  padding-top: 23rpx;
  padding-bottom: 21rpx;
}

.res-lists-i-actually-p{
  font-size: 28rpx;
  color: #292621;
}

.res-lists-i-line{
  padding-left: 30rpx;
  padding-right: 30rpx;
  width: 100%;
  border-bottom: 1px solid #F5F2ED;
}

.res-lists-i-btn-wrap{
  padding: 20rpx 24rpx 16rpx 24rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.i-btn{
  width: 100rpx;
  height: 44rpx;
  font-size: 24rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 16rpx;
}

.i-btn-cancel{
  width: 126rpx;
  border: 1px solid #E7E7E7;
  color: #E7E7E7;
}

.i-btn-payment{
  background: #FF4648;
  color: #fff;
}

.i-btn-check{
  background: #FF4648;
  color: #fff;
}

.res-lists-footer{
  margin-top: 55rpx;
  margin-bottom: 55rpx;
  color: #909090;
  font-size: 24rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.footer-l{
  width: 40rpx;
  border-bottom: 1px solid #6A7B86;
}

.footer-txt{
  margin-left: 16rpx;
  margin-right: 16rpx;
}